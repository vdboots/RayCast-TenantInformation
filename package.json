{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tenant-information",
  "title": "Tenant information",
  "description": "Microsoft Azure AD tenant information",
  "icon": "command-icon.png",
  "author": "vdboots",
  "owner": "innovative",
  "license": "MIT",
  "commands": [
    {
      "name": "copy",
      "title": "Copy",
      "subtitle": "copy Microsoft tenant ID",
      "description": "copies the microsoft Azure AD tenant id",
      "mode": "no-view",
      "arguments": [
        {
          "name": "domain",
          "placeholder": "domainname",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8",
    "@raycast/utils": "^1.5.2",
    "guid-typescript": "^1.0.9",
    "node-fetch": "^3.3.1",
    "raycast": "^1.0.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
